{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": ["warn", { "varsIgnorePattern": "React" }],
    "react/jsx-uses-vars": "error",
    "react/sort-prop-types": "error",
    "react/prop-types": "error",
    "arrow-parens": off,
    "object-curly-newline": off,
    "import/no-unresolved": [2, { "ignore": ["^@+"] }]
  },
  "extends": ["airbnb-base"]
}
